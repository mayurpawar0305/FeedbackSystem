@using BOL;
@{
    ViewBag.Title = "Insert";
}

<h2>Insert Feedback</h2>
<hr />
<a href="/home/index">Home</a> |
<a href="/Student/index">Student Details</a>
<hr />
<form method="post" action="/student/insert">
    <div class="form-group">
        <label for="id">Feedback ID:</label>
        <input type="number" class="form-control" id="id" name="id" />
    </div>
    <div class="form-group">
        <label for="studentname">Student Name:</label>
        <input type="text" class="form-control" id="studentname" name="studentname" />
    </div>
    <div class="form-group">
        <label for="module">Module:</label>
        <input type="text" class="form-control" id="module" name="module" />
    </div>
    <div class="form-group">
        <label for="faculty">Faculty:</label>
        <input type="text" class="form-control" id="faculty" name="faculty" />
    </div>
    <div class="form-group">
        <label for="problemsolvingrating">Problem Solving Rating:</label>
        <input type="number" class="form-control" id="problemsolvingrating" name="problemsolvingrating" />
    </div>
    <div class="form-group">
        <label for="presentationskill">Presentation Skill:</label>
        <input type="number" class="form-control" id="presentationskill" name="presentationskill" />
    </div>
    <div class="form-group">
        <label for="comments">Comments:</label>
        <input type="text" class="form-control" id="comments" name="comments" />
    </div>
    <div class="form-group">
        <label for="date">Date:</label>
        <input type="datetime" class="form-control" id="date" name="date" />
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
</form>
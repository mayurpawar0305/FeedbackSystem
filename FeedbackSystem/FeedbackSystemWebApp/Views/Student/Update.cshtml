@using BOL;
@{
    ViewBag.Title = "Update";
}

<h2>Update Feedback Details</h2>
<form method="post" action="/student/update">
    <div class="form-group">
        <label for="id">Feedback ID:</label>
        <input type="number" class="form-control" id="id" name="id" value="@Model.FeedbackId"/>
    </div>
    <div class="form-group">
        <label for="studentname">Student Name:</label>
        <input type="text" class="form-control" id="studentname" name="studentname" value="@Model.StudentName"/>
    </div>
    <div class="form-group">
        <label for="module">Module:</label>
        <input type="text" class="form-control" id="module" name="module" value="@Model.Module"/>
    </div>
    <div class="form-group">
        <label for="faculty">Faculty:</label>
        <input type="text" class="form-control" id="faculty" name="faculty" value="@Model.Faculty"/>
    </div>
    <div class="form-group">
        <label for="problemsolvingrating">Problem Solving Rating:</label>
        <input type="number" class="form-control" id="problemsolvingrating" name="problemsolvingrating" value="@Model.ProblemSolvingRating"/>
    </div>
    <div class="form-group">
        <label for="presentationskill">Presentation Skill:</label>
        <input type="number" class="form-control" id="presentationskill" name="presentationskill" value="@Model.PresentationSkill"/>
    </div>
    <div class="form-group">
        <label for="comments">Comments:</label>
        <input type="text" class="form-control" id="comments" name="comments" value="@Model.Comments"/>
    </div>
    <div class="form-group">
        <label for="date">Date:</label>
        <input type="datetime" class="form-control" id="date" name="date" value="@Model.Date"/>
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
</form>
